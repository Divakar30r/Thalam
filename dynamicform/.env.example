# Vite environment variables. Copy to .env.local and adjust for your environment.
VITE_API_BASE_URL=http://127.0.0.1:8001
# Optional: override the presign endpoint and the config name used by the backend
# e.g. VITE_PRESIGNED_URL_ENDPOINT=https://bff.example.com/api/v1/dochandler/presigned
VITE_PRESIGNED_URL_ENDPOINT=http://localhost:8000/api/v1/presigned-upload/
# Optional friendly config name sent with presign request (defaults to 'PRESIGNED URL')
VITE_PRESIGNED_CONFIG_NAME=

# Keycloak (used by the auth backend example)
# KEYCLOAK_BASE: base URL of your Keycloak server (no trailing slash)
KEYCLOAK_BASE=https://keycloak.drapps.dev
# KEYCLOAK_REALM: realm name
KEYCLOAK_REALM=OrderMgmt
# Client credentials for the ORDMGMT client (server-side secret optional)
KEYCLOAK_ORDMGMT_CLIENT_ID=ORDMGMT
KEYCLOAK_ORDMGMT_CLIENT_SECRET=j
# Client id used for checking DYNAMICFORM roles
KEYCLOAK_DYNAMICFORM_CLIENT_ID=DYNAMICFORM
KEYCLOAK_REDIRECT_URI=http://localhost:8002/auth/callback
DISABLE_JWT_VERIFY=false
