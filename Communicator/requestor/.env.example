# Requestor Service Environment Variables

# Service Configuration
SERVICE_NAME=requestor
DEBUG_MODE=false
LOG_LEVEL=INFO

# FastAPI Configuration
FASTAPI_PORT=8004
CORS_ORIGINS=["*"]

# API URLs
API_BASE_URL=http://localhost:8001
API_GOOGLE_BOT_URL=http://localhost:8006
API_GOOGLE_CHAT_URL=http://localhost:8007

# gRPC Configuration
PROCESSOR_GRPC_HOST=localhost
PROCESSOR_GRPC_PORT=50051
GRPC_REQUEST_TIMEOUT=30
MAX_STREAMING_CONNECTIONS=5
STREAMING_RECONNECT_DELAY=5

# Kafka/MSK Configuration
KAFKA_BOOTSTRAP_SERVERS=["localhost:9092"]

# HTTP Client Configuration
HTTP_TIMEOUT=30

# Queue Management
MAX_CONCURRENT_TASKS=10
ORDER_EXPIRY_MINUTES=30

# Seller Selection
FIND_MAX_SEL=10